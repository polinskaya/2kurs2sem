<?xml version="1.0" encoding="utf-8"?>
<!-- 
    Этот файл служит для блокировки доступа к файлам шаблонов, которые
    не должны запрашиваться напрямую.
-->
<configuration>
    <system.web>
      <httpHandlers>
        <add path="*.aspx" verb="*" type="System.Web.HttpNotFoundHandler" validate="true"/>
      </httpHandlers>
    </system.web>
</configuration>