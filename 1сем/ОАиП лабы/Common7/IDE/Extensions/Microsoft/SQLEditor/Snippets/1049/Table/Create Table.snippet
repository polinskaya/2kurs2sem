<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <_locDefinition xmlns="urn:locstudio">
    <_locDefault _loc="locNone"/>
    <_locTag _loc="locData">Title</_locTag>
    <_locTag _loc="locData">Description</_locTag>
    <_locTag _loc="locData">Author</_locTag>
    <_locTag _loc="locData">ToolTip</_locTag>
  </_locDefinition>
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Создать таблицу</Title>
      <Shortcut></Shortcut>
      <Description>Создает таблицу.</Description>
      <Author>Корпорация Майкрософт</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>SchemaName</ID>
          <ToolTip>Имя схемы</ToolTip>
          <Default>dbo</Default>
        </Literal>
        <Literal>
          <ID>Tablename</ID>
          <ToolTip>Имя таблицы</ToolTip>
          <Default>Sample_Table</Default>
        </Literal>
        <Literal>
          <ID>column1</ID>
          <ToolTip>Имя столбца</ToolTip>
          <Default>column_1</Default>
        </Literal>
        <Literal>
          <ID>datatype1</ID>
          <ToolTip>Тип данных столбца</ToolTip>
          <Default>int NOT NULL</Default>
        </Literal>
        <Literal>
          <ID>column2</ID>
          <ToolTip>Имя столбца</ToolTip>
          <Default>column_2</Default>
        </Literal>
        <Literal>
          <ID>datatype2</ID>
          <ToolTip>Тип данных столбца</ToolTip>
          <Default>int NULL</Default>
        </Literal>
      </Declarations>
      <Code Language="SQL_SSDT">
        <![CDATA[CREATE TABLE $SchemaName$.$Tablename$
(
    $column1$ $datatype1$,
    $column2$ $datatype2$
);$end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
