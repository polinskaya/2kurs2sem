<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <_locDefinition xmlns="urn:locstudio">
    <_locDefault _loc="locNone"/>
    <_locTag _loc="locData">Title</_locTag>
    <_locTag _loc="locData">Description</_locTag>
    <_locTag _loc="locData">Author</_locTag>
    <_locTag _loc="locData">ToolTip</_locTag>
  </_locDefinition>
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Создать процедуру с параметром OUTPUT</Title>
      <Shortcut></Shortcut>
      <Description>Создает хранимую процедуру, имеющую выходной параметр.</Description>
      <Author>Корпорация Майкрософт</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>SchemaName</ID>
          <ToolTip>Имя схемы</ToolTip>
          <Default>dbo</Default>
        </Literal>
        <Literal>
          <ID>storedprocname</ID>
          <ToolTip>Имя хранимой процедуры</ToolTip>
          <Default>Sample_Procedure</Default>
        </Literal>
        <Literal>
          <ID>Param1</ID>
          <ToolTip>Имя входного параметра</ToolTip>
          <Default>param1</Default>
        </Literal>
        <Literal>
          <ID>datatype1</ID>
          <ToolTip>Тип данных входного параметра</ToolTip>
          <Default>int</Default>
        </Literal>
        <Literal>
          <ID>DefValue1</ID>
          <ToolTip>Значение входного параметра по умолчанию</ToolTip>
          <Default>0</Default>
        </Literal>
        <Literal>
          <ID>Param2</ID>
          <ToolTip>Имя выходного параметра</ToolTip>
          <Default>param2</Default>
        </Literal>
        <Literal>
          <ID>datatype2</ID>
          <ToolTip>Тип данных выходного параметра </ToolTip>
          <Default>int</Default>
        </Literal>
      </Declarations>
      <Code Language="SQL_SSDT">
        <![CDATA[CREATE PROCEDURE $SchemaName$.$storedprocname$ 
    @$Param1$ $datatype1$ = $DefValue1$,
    @$Param2$ $datatype2$ OUTPUT 
AS
    SELECT @$Param2$ = @$Param2$ + @$Param1$ 
RETURN 0 $end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
